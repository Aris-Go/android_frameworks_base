[project]
	description = Android framework classes and services
[receive]
	requireContributorAgreement = true
[submit]
	action = merge always
	mergeContent = true
[mimetype "text/x-java"]
	path = api/current.txt
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group approvers-platform-frameworks-base
	label-Code-Review = -2..+1 group Android Maintainers
	label-Verified = -1..+1 group AOSP
	label-Verified = -1..+0 group Android Maintainers
[access "refs/heads/master"]
	label-Verified = -1..+1 group Android Maintainers
	submit = group Android Maintainers
[access]
	inheritFrom = api_council_filter
