[project]
	description = Android framework classes and services
[receive]
	requireContributorAgreement = true
[submit]
	action = merge always
	mergeContent = true
[mimetype "text/x-java"]
	path = api/current.txt
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group Administrators
	label-Code-Review = -2..+2 group approvers-platform-frameworks-base
	label-Code-Review = -2..+1 group Android Maintainers
	label-Verified = -1..+1 group AOSP
	label-Verified = -1..+0 group Android Maintainers
[access "refs/heads/master"]
	label-Verified = -1..+1 group Android Maintainers
	submit = group Android Maintainers
[access]
	inheritFrom = Platform-Projects
[access "refs/heads/stage-telephony-refactor"]
	create = group mdb/chromeos-test
	forgeAuthor = group mdb/chromeos-test
	forgeCommitter = group mdb/chromeos-test
	push = group mdb/chromeos-test
	pushMerge = group mdb/chromeos-test
[access "^refs/heads/.*-gsi"]
	label-Code-Review = -2..+2 group gsi-approvers
