java_library_static {
    name: "framework-protos",
    host_supported: true,
    proto: {
        type: "nano",
    },
    srcs: ["src/**/*.proto"],
    no_framework_libs: true,
    target: {
        android: {
            // Decouple repackaged targets from toolchain version. http://b/72703434
            java_version: "1.8",
            jarjar_rules: "jarjar-rules.txt",
        },
        host: {
            static_libs: ["libprotobuf-java-nano"],
        },
    },
}
