package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

java_library {
    name: "annotations_via_static_libs",
    installable: true,
    srcs: ["TestAnnotations.java"],
    optimize: {
        enabled: true,
        shrink: true,
    },
    static_libs: ["androidx.annotation_annotation"],
}

java_library {
    name: "annotations_via_libs",
    installable: true,
    srcs: ["TestAnnotations.java"],
    optimize: {
        enabled: true,
        shrink: true,
    },
    libs: ["androidx.annotation_annotation"],
}

java_library {
    name: "annotations_via_libs_nodeps",
    installable: true,
    srcs: ["TestAnnotations.java"],
    optimize: {
        enabled: true,
        shrink: true,
        proguard_flags_files: [
            ":androidx.annotation_annotation-nodeps{.aapt.proguardOptionsFile}",
        ],
    },
    libs: ["androidx.annotation_annotation-nodeps"],
}
