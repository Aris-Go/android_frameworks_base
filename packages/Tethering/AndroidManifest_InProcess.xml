<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.tethering.inprocess"
          android:sharedUserId="android.uid.system"
          android:process="system">
    <uses-sdk android:minSdkVersion="28" android:targetSdkVersion="28" />
    <!-- Remove below use permission when using PlatformNetworkPermissionConfig.
    <uses-permission android:name="android.permission.CONNECTIVITY_INTERNAL" />
    <uses-permission android:name="android.permission.NETWORK_STACK" /> -->
    <application>
        <service android:name="com.android.server.connectivity.TetheringService"
                 android:process="system"
                 android:permission="android.permission.NETWORK_STACK">
            <intent-filter>
                <action android:name="android.net.ITetheringConnector.InProcess"/>
            </intent-filter>
        </service>
    </application>
</manifest>
