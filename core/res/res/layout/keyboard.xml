<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/view_keyboard_root" android:orientation="vertical"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:gravity="bottom">

	<LinearLayout android:id="@+id/keyboard_phone_misc"
		android:orientation="vertical" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:gravity="bottom"
		android:background="#7f000000">

		<LinearLayout android:id="@+id/keyboard_misc_line_1"
			android:orientation="horizontal" android:layout_width="fill_parent"
			android:layout_height="70dp" android:gravity="center_horizontal">

			<Button android:id="@+id/home" android:text="@string/keyboard_home"
				android:tag="3" android:layout_width="50dp" android:layout_height="wrap_content"
				android:layout_weight="1.0" android:textStyle="bold" />

			<Button android:id="@+id/call" android:text="@string/keyboard_call"
				android:tag="5" android:layout_width="70dp" android:layout_height="70dp"
				android:textStyle="bold" android:textColor="#00ff00"
				android:textSize="40dp" />

			<Button android:id="@+id/dpad_up" android:text="@string/keyboard_dpad_up"
				android:tag="19" android:layout_width="50dp" android:layout_height="wrap_content"
				android:textStyle="bold" />

			<Button android:id="@+id/endcall" android:text="@string/keyboard_endcall"
				android:tag="6" android:layout_width="70dp" android:layout_height="70dp"
				android:textStyle="bold" android:textColor="#ff0000"
				android:textSize="40dp" />

			<Button android:id="@+id/vol_up" android:text="@string/keyboard_vol_up"
				android:tag="24" android:layout_width="50dp" android:layout_height="wrap_content"
				android:layout_weight="1.0" android:textStyle="bold" />

		</LinearLayout>

		<LinearLayout android:id="@+id/keyboard_misc_line_2"
			android:orientation="horizontal" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:gravity="center">

			<Button android:id="@+id/back" android:text="@string/keyboard_back"
				android:tag="4" android:layout_width="50dp" android:layout_height="wrap_content"
				android:layout_weight="1.0" android:textStyle="bold" />

			<Button android:id="@+id/dpad_left" android:text="@string/keyboard_dpad_left"
				android:tag="21" android:layout_width="50dp" android:layout_height="wrap_content"
				android:textStyle="bold" />

			<Button android:id="@+id/dpad_center" android:text="@string/keyboard_dpad_center"
				android:tag="23" android:layout_width="50dp" android:layout_height="wrap_content"
				android:textStyle="bold" />

			<Button android:id="@+id/dpad_right" android:text="@string/keyboard_dpad_right"
				android:tag="22" android:layout_width="50dp" android:layout_height="wrap_content"
				android:textStyle="bold" />

			<Button android:id="@+id/vol_down" android:text="@string/keyboard_vol_down"
				android:tag="25" android:layout_width="50dp" android:layout_height="wrap_content"
				android:layout_weight="1.0" android:textStyle="bold" />

		</LinearLayout>

		<LinearLayout android:id="@+id/keyboard_misc_line_3"
			android:orientation="horizontal" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:gravity="center">

			<Button android:id="@+id/swap_gravity_3" android:text="@string/keyboard_swap_gravity"
				android:layout_width="50dp" android:layout_height="wrap_content"
				android:textStyle="bold" />

			<Button android:id="@+id/toggle_view_3" android:text="@string/keyboard_toggle_view"
				android:layout_width="50dp" android:layout_height="wrap_content"
				android:layout_weight="1.0" android:textStyle="bold" />

			<Button android:id="@+id/dpad_down" android:text="@string/keyboard_dpad_down"
				android:tag="20" android:layout_width="50dp" android:layout_height="wrap_content"
				android:textStyle="bold" />

			<Button android:id="@+id/clear" android:text="@string/keyboard_clear"
				android:tag="28" android:layout_width="50dp" android:layout_height="wrap_content"
				android:layout_weight="1.0" android:textStyle="bold" />

			<Button android:id="@+id/hide_3" android:text="@string/keyboard_hide"
				android:layout_width="50dp" android:layout_height="wrap_content"
				android:textStyle="bold" />

		</LinearLayout>

	</LinearLayout>

