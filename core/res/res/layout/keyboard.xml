		<LinearLayout android:orientation="horizontal"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center_horizontal">

			<Button android:id="@+id/shift_left_2" android:text="@string/keyboard_shift_left"
				android:tag="59" android:layout_width="50dp" android:layout_height="wrap_content"
